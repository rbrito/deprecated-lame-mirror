				;;
			esac
			;;
		*)
			# no special optimization for other versions
			{ echo "$as_me:$LINENO: WARNING: LAME doesn't know about your version of gcc" >&5
echo "$as_me: WARNING: LAME doesn't know about your version of gcc" >&2;}
			;;
